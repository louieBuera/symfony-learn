{% extends 'base.html.twig' %}

{% block title %}Hello ProductController!{% endblock %}

{% block body %}

<h1>Product</h1>

<table>
    <tbody>
      <tr>
          <td>Name</td>
          <td>{{ product.name }}</td>
      </tr>
      <tr>
          <td>Description</td>
          <td>{{ product.description }}</td>
      </tr>
      <tr>
          <td>Size</td>
          <td>{{ product.size }}</td>
      </tr>
    </tbody>
</table>

{% endblock %}
